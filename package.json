{
  "name": "component-party.dev",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "repository": "github:matschik/component-party.dev",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "_format": "prettier --cache --cache-location .cache/prettier",
    "format": "pnpm _format --write .",
    "lint": "pnpm format && oxlint && eslint",
    "check": "pnpm format && svelte-check --tsconfig ./tsconfig.json && pnpm lint && pnpm test",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "build:content": "node scripts/generateContent.ts",
    "build:progress": "node scripts/generateReadMeProgress.ts",
    "prepare": "lefthook install",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install chromium",
    "test": "pnpm run test:e2e",
    "check:package-manager": "node scripts/check-package-manager.js"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "just-throttle": "^4.2.0",
    "runed": "^0.31.1",
    "sv-router": "^0.9.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.36.0",
    "@iconify/json": "^2.2.386",
    "@iconify/tailwind4": "^1.0.6",
    "@matschik/lz-string": "^0.0.2",
    "@playwright/test": "^1.55.0",
    "@shikijs/markdown-it": "^3.13.0",
    "@sveltejs/vite-plugin-svelte": "6.2.0",
    "@sveltejs/vite-plugin-svelte-inspector": "5.0.1",
    "@tailwindcss/vite": "^4.1.13",
    "@types/folder-hash": "^4.0.4",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/markdown-it": "^14.1.2",
    "autoprefixer": "^10.4.21",
    "chokidar": "^4.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "^1.16.0",
    "eslint-plugin-svelte": "^3.12.3",
    "eta": "^4.0.1",
    "folder-hash": "^4.1.1",
    "globals": "^16.4.0",
    "html-minifier-terser": "^7.2.0",
    "just-kebab-case": "^4.2.0",
    "lefthook": "^1.13.1",
    "markdown-it": "^14.1.0",
    "micache": "^2.4.1",
    "oxlint": "^1.16.0",
    "package-directory": "^8.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "shiki": "^3.13.0",
    "svelte": "5.39.3",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.13",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.6",
    "vite-plugin-html": "^3.2.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
